{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "css.customData": [
        ".vscode/css_custom_data.json"
    ],
    "files.associations": {
        "*.css": "tailwindcss"
    }
}